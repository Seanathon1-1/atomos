# Alternative GNU Make project makefile autogenerated by Premake

ifndef config
  config=debug_win32
endif

ifndef verbose
  SILENT = @
endif

.PHONY: clean prebuild

SHELLTYPE := posix
ifeq (.exe,$(findstring .exe,$(ComSpec)))
	SHELLTYPE := msdos
endif

# Configurations
# #############################################

INCLUDES += -Isrc -I../Atomos
FORCE_INCLUDE +=
ALL_CPPFLAGS += $(CPPFLAGS) $(DEFINES) $(INCLUDES)
ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
LINKCMD = $(CXX) -o "$@" $(OBJECTS) $(RESOURCES) $(ALL_LDFLAGS) $(LIBS)
define PREBUILDCMDS
endef
define PRELINKCMDS
endef
define POSTBUILDCMDS
endef

ifeq ($(config),debug_win32)
TARGETDIR = ../bin/bin/Debug/x86/Balls
TARGET = $(TARGETDIR)/Balls.exe
OBJDIR = obj/Win32/Debug
DEFINES += /D"_UNICODE" /D"UNICODE" /D"DEBUG"
ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) /MDd /Z7 --std=c++2a -M
ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) /MDd /Z7 /EHsc --std=c++2a -M
LIBS += ../bin/bin/Debug/x86/Atomos/Atomos.lib
LDDEPS += ../bin/bin/Debug/x86/Atomos/Atomos.lib
ALL_LDFLAGS += $(LDFLAGS) /NOLOGO /DEBUG

else ifeq ($(config),debug_x64)
TARGETDIR = ../bin/bin/Debug/x86_64/Balls
TARGET = $(TARGETDIR)/Balls.exe
OBJDIR = obj/x64/Debug
DEFINES += /D"_UNICODE" /D"UNICODE" /D"DEBUG"
ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) /MDd /Z7 --std=c++2a -M
ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) /MDd /Z7 /EHsc --std=c++2a -M
LIBS += ../bin/bin/Debug/x86_64/Atomos/Atomos.lib
LDDEPS += ../bin/bin/Debug/x86_64/Atomos/Atomos.lib
ALL_LDFLAGS += $(LDFLAGS) /NOLOGO /DEBUG

else ifeq ($(config),release_win32)
TARGETDIR = ../bin/bin/Release/x86/Balls
TARGET = $(TARGETDIR)/Balls.exe
OBJDIR = obj/Win32/Release
DEFINES += /D"_UNICODE" /D"UNICODE" /D"NDEBUG"
ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) /Ot /MD --std=c++2a -M
ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) /Ot /MD /EHsc --std=c++2a -M
LIBS += ../bin/bin/Release/x86/Atomos/Atomos.lib
LDDEPS += ../bin/bin/Release/x86/Atomos/Atomos.lib
ALL_LDFLAGS += $(LDFLAGS) /NOLOGO

else ifeq ($(config),release_x64)
TARGETDIR = ../bin/bin/Release/x86_64/Balls
TARGET = $(TARGETDIR)/Balls.exe
OBJDIR = obj/x64/Release
DEFINES += /D"_UNICODE" /D"UNICODE" /D"NDEBUG"
ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) /Ot /MD --std=c++2a -M
ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) /Ot /MD /EHsc --std=c++2a -M
LIBS += ../bin/bin/Release/x86_64/Atomos/Atomos.lib
LDDEPS += ../bin/bin/Release/x86_64/Atomos/Atomos.lib
ALL_LDFLAGS += $(LDFLAGS) /NOLOGO

endif

# Per File Configurations
# #############################################

PERFILE_FLAGS_0 = $(ALL_CXXFLAGS) /MD /D"_UNICODE" /D"UNICODE"

# File sets
# #############################################

GENERATED :=
OBJECTS :=

GENERATED += $(OBJDIR)/Balls.o
OBJECTS += $(OBJDIR)/Balls.o

# Rules
# #############################################

all: $(TARGET)
	@:

$(TARGET): $(GENERATED) $(OBJECTS) $(LDDEPS) | $(TARGETDIR)
	$(PRELINKCMDS)
	@echo Linking Balls
	$(SILENT) $(LINKCMD)
	$(POSTBUILDCMDS)

$(TARGETDIR):
	@echo Creating $(TARGETDIR)
ifeq (posix,$(SHELLTYPE))
	$(SILENT) mkdir -p $(TARGETDIR)
else
	$(SILENT) mkdir $(subst /,\\,$(TARGETDIR))
endif

$(OBJDIR):
	@echo Creating $(OBJDIR)
ifeq (posix,$(SHELLTYPE))
	$(SILENT) mkdir -p $(OBJDIR)
else
	$(SILENT) mkdir $(subst /,\\,$(OBJDIR))
endif

clean:
	@echo Cleaning Balls
ifeq (posix,$(SHELLTYPE))
	$(SILENT) rm -f  $(TARGET)
	$(SILENT) rm -rf $(GENERATED)
	$(SILENT) rm -rf $(OBJDIR)
else
	$(SILENT) if exist $(subst /,\\,$(TARGET)) del $(subst /,\\,$(TARGET))
	$(SILENT) if exist $(subst /,\\,$(GENERATED)) del /s /q $(subst /,\\,$(GENERATED))
	$(SILENT) if exist $(subst /,\\,$(OBJDIR)) rmdir /s /q $(subst /,\\,$(OBJDIR))
endif

prebuild: | $(OBJDIR)
	$(PREBUILDCMDS)

ifneq (,$(PCH))
$(OBJECTS): $(GCH) | $(PCH_PLACEHOLDER)
$(GCH): $(PCH) | prebuild
	@echo $(notdir $<)
	$(SILENT) $(CXX) -x c++-header $(ALL_CXXFLAGS) -o "$@" -MF "$(@:%.gch=%.d)" -c "$<"
$(PCH_PLACEHOLDER): $(GCH) | $(OBJDIR)
ifeq (posix,$(SHELLTYPE))
	$(SILENT) touch "$@"
else
	$(SILENT) echo $null >> "$@"
endif
else
$(OBJECTS): | prebuild
endif


# File Rules
# #############################################

$(OBJDIR)/Balls.o: src/Balls.cpp
	@echo "$(notdir $<)"
	$(SILENT) $(CXX) $(PERFILE_FLAGS_0) $(FORCE_INCLUDE) -o "$@" -MF "$(@:%.o=%.d)" -c "$<"

-include $(OBJECTS:%.o=%.d)
ifneq (,$(PCH))
  -include $(PCH_PLACEHOLDER).d
endif